{
  "name": "next-blog",
  "private": true,
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "db:push": "dotenv -e .env.development.local --- npx prisma db push",
    "db:migrate": "dotenv -e .env.development.local --- npx prisma migrate dev",
    "db:seed": "dotenv -e .env.development.local --- npx prisma db seed",
    "db:reset": "dotenv -e .env.development.local --- npx prisma migrate reset"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.3",
    "@heroicons/react": "^1.0.6",
    "dayjs": "^1.11.2",
    "lodash.debounce": "^4.0.8",
    "next": "^13.0.1",
    "next-mdx-remote": "^4.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^9.0.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-code-titles": "^1.0.3",
    "rehype-highlight": "^5.0.2",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.1",
    "swr": "^1.3.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "@types/lodash.debounce": "^4",
    "@types/node": "17.0.17",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/validator": "^13",
    "dotenv-cli": "^5.1.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "postcss": "^8.4.6",
    "prisma": "latest",
    "tailwindcss": "^3.0.19",
    "typescript": "4.5.5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
